projek rumah sakit

menu: (
	1. menu registrasi pasien
	2. menu list dokter berdasarkan poli nya
	2. menu daftar poli tertentu with payment status
	3. menu daftar rawat inap with payment status
	4. menu daftar lab with payment status
)

object model attr requirements: (
	pasien: (
		0. pasien.objectName
		1. pasien.id
		2. data diri (nama usia tanggal lahir alamat)
		3. have_asuransi?
		4. nama asuransi && no asuransi ? 3 = true
		5. room_assigned (default: none)
		6. tgl_masukRawat
		7. tgl_keluar
	)
	dokter: (
		0. dokter.objectName
		1. dokter.id
		2. data diri(nama usia tanggal lahir)
		3. penempatan poli (dan jadwal?) many2one
	)
	poli : (
		1. poli.objectName(for id)
		2. Nama poli
		3. dokter yang terdaftar pada poli tsb one2many
		4. nama pasien yg mendaftar poli
		5. tgl_mendaftar(default = date.now)
		4. payment status(awaiting payment, on verification, confirmed)
	)
	3. rawat inap : (
		0. poli.objectName
		1. rawatInap.id
		2. nomor ruangan
		3. kelas ruangan (3, 2, 1, vip) one2many
		4. nama pasien yg terdaftar di ruangan tsb many2one
		5. payment status(awaiting payment, on verification, confirmed)
	)
	4. lab : (
		0. lab.objectName
		1. nama lab
		2. nama pasien yg mendaftar lab
		3. tgl_mendaftar(default = date.now)
		4. payment status(awaiting payment, on verification, confirmed)
	)
)